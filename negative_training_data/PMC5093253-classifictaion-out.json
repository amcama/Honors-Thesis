}, {
  "sentence_tokens" : [ "The", "median", "survival", "time", "for", "dogs", "without", "azotemia", "was", "39", "months", "(", "95", "%", "CI", ":", "20.4", "-", "57.5", ")", "and", "for", "those", "with", "azotemia", "was", "20", "months", "(", "95", "%", "CI", ":", "14.5", "-", "25.5", ";", "p", "=", "0.173", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "median", "survival", "time", "for", "dogs", "with", "blood", "creatinine", "<=", "1.6", "mg/dL", "was", "43", "months", "(", "95", "%", "CI", ":", "18.2", "-", "67.5", ")", "and", "for", "those", "with", "blood", "creatinine", ">", "1.6", "mg/dL", "was", "20", "months", "(", "95", "%", "CI", ":", "10.5", "-", "29.5", ";", "p", "=", "0.159", ")", "." ]
}, {
