}, {
  "sentence_tokens" : [ "Discussion", "." ]
}, {
  "sentence_tokens" : [ "In", "this", "study", ",", "we", "observed", "the", "diagnostic", "performance", "of", "QFR", "in", "all", "comer", "patients", "who", "underwent", "FFR", "not", "only", "in", "the", "angina", "population", "but", "also", "in", "the", "AMI", "population", "with", "noninfarct", "related", "arteries", "." ]
