}, {
  "sentence_tokens" : [ "The", "diagnostic", "accuracy", "of", "the", "QFR", "with", "the", "FFR", "as", "a", "reference", "in", "our", "work", "was", "95.54", "%", ",", "which", "is", "the", "highest", "accuracy", "reported", "among", "existing", "studies", ",", "possibly", "because", "our", "2", "investigators", "assessed", "the", "QFR", "after", "training", "with", "more", "than", "300", "patients", "each", "." ]
}, {
  "sentence_tokens" : [ "After", "QFR", "analysis", "training", ",", "we", "conducted", "a", "pilot", "study", "(", "110", "cases", ")", "before", "entering", "the", "QFR", "analysis", "of", "this", "registry", "data", "." ]
}, {
  "sentence_tokens" : [ "The", "interobserver", "variability", "of", "QFR", "were", "assessed", "using", "the", "intraclass", "correlation", "coefficient", "(", "ICC", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "ICC", "between", "the", "two", "observers", "was", "0.90", "(", "95", "%", "CI", ":", "0.82", "to", "0.94", ")", "indicated", "excellent", "agreement", "." ]
}, {
  "sentence_tokens" : [ "Another", "possible", "reason", "for", "the", "good", "results", "in", "our", "study", "is", "that", "34.8", "%", "of", "the", "evaluated", "vessels", "had", "PAS", "< 50%_or >", "70", "%", "which", "may", "have", "resulted", "in", "increased", "diagnostic", "power", "(", "Supplemental", "Fig.", "1", ")", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "this", "observation", "cannot", "explain", "all", "the", "results", "because", "the", "incidence", "of", "vessels", "(", "37.8", "%", ")", "with", "an", "FFR", "<=", "0.8", "was", "comparable", "to", "that", "in", "previous", "studies", "(", "33", "%", "and", "34", "%", ")", "." ]
}, {
  "sentence_tokens" : [ "Importantly", ",", "this", "study", "includes", "comparison", "data", "of", "the", "diagnostic", "performance", "of", "the", "QFR", "for", "identifying", "an", "FFR", "of", "<=", "0.8", "in", "the", "AMI", "and", "angina", "groups", "(", "Fig.", "3", ")", "." ]
}, {
  "sentence_tokens" : [ "Until", "recently", ",", "the", "diagnostic", "performance", "of", "QFR", "was", "mostly", "assessed", "in", "patients", "with", "stable", "coronary", "disease", "." ]
}, {
  "sentence_tokens" : [ "Concern", "regarding", "the", "variability", "of", "QFR", "values", "under", "AMI", "conditions", "has", "emerged", "because", "the", "calculation", "is", "based", "on", "the", "assumed", "flow", "velocity", "with", "frame", "counting", ",", "which", "can", "be", "affected", "by", "microvascular", "resistance", "." ]
}, {
  "sentence_tokens" : [ "The", "representative", "cases", "reflecting", "this", "hypothesis", "are", "briefly", "and", "prominently", "presented", "in", "Fig.", "5", "." ]
}, {
  "sentence_tokens" : [ "Case", "1", "describes", "that", "the", "QFR", "value", "can", "be", "mismatched", "with", "the", "FFR", "in", "vessels", "that", "are", "noninfarct", "related", "arteries", "in", "AMI", "conditions", "and", "contain", "multiple", "lesion", "complexities", ",", "such", "as", "tandem", "long", "bifurcation", "lesions", "." ]
}, {
  "sentence_tokens" : [ "Case", "2", "describes", "that", "QFR", "matches", "well", "with", "FFR", "values", "and", "has", "very", "high", "diagnostic", "power", "in", "patients", "with", "only", "single", "short", "lesions", "." ]
}, {
  "sentence_tokens" : [ "QFR", "analysis", "applies", "correction", "values", "using", "flow", "velocity", "information", "by", "the", "frame", "count", "method", "as", "well", "as", "geometry", "information", "of", "vessels", "." ]
}, {
  "sentence_tokens" : [ "A", "low", "flow", "velocity", "may", "indicate", "high", "microvascular", "resistance", "and", "damage", ",", "especially", "in", "AMI", "conditions.From", "our", "cohort", ",", "we", "observed", "that", "the", "accuracy", "of", "the", "QFR", "in", "the", "AMI", "group", "was", "relatively", "lower", "than", "that", "in", "the", "angina", "group", "without", "a", "significant", "difference", "(", "92.42", "%", "[", "95", "%", "CI", "86.51", "to", "96.31", "%", "]", "vs.", "95.98", "%", "[", "95", "%", "CI", "94.52", "to", "97.14", "%", "]", ";", "difference", "3.55", ";", "p", "=", "0.064", ")", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "in", "the", "conventional", "'", "borderline", "FFR", "zone", "'", "group", "(", "FFR", "0.75", "-", "0.80", ")", "accompanying", "AMI", ",", "the", "diagnostic", "accuracy", "of", "the", "QFR", "for", "FFR", "was", "significantly", "lower", "than", "that", "in", "the", "total", "population", "with", "angina", "." ]
}, {
  "sentence_tokens" : [ "(", "83.93", "%", "[", "95", "%", "CI", "71.67", "to", "92.38", "%", "]", ";", "difference", "12.05", "[", "95", "%", "CI", "2.35", "to", "21.75", "]", ";", "p", "=", "0.001", ")", "(", "Fig.", "3", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "condition", "accompanying", "both", "AMI", "and", "the", "borderline", "FFR", "zone", "showed", "the", "lowest", "QFR", "diagnostic", "accuracy", "in", "our", "data", "." ]
}, {
  "sentence_tokens" : [ "These", "outcomes", "are", "consistent", "with", "the", "results", "of", "previous", "studies", "." ]
}, {
  "sentence_tokens" : [ "Under", "AMI", "conditions", ",", "transient", "microvascular", "dysfunction", "may", "occur", "due", "to", "microembolization", ",", "inflammation", ",", "myocardial", "edema", ",", "or", "necrosis", ",", "especially", "when", "the", "no-reflow", "phenomenon", "occurs", "." ]
}, {
  "sentence_tokens" : [ "This", "microvascular", "damage", "is", "not", "limited", "to", "the", "infarct", "related", "artery", "but", "may", "also", "extend", "to", "the", "noninfarct", "related", "artery", "territory", "." ]
}, {
  "sentence_tokens" : [ "In", "previous", "studies", ",", "lower", "intracoronary", "measured", "coronary", "flow", "velocity", "reserve", "and", "higher", "minimal", "microvascular", "resistance", "were", "observed", "in", "the", "infarcted", "and", "noninfarcted", "regions", "during", "AMI", "than", "at", "follow-up", "." ]
}, {
  "sentence_tokens" : [ "Besides", ",", "this", "may", "result", "in", "the", "low", "diagnostic", "accuracy", "of", "QFR", "to", "FFR", "for", "AMI", "patients.We", "suggest", "a", "QFR-FFR", "hybrid", "approach", "for", "these", "subsets", "with", "low", "diagnostic", "power", "(", "Fig.", "6", ")", "." ]
}, {
  "sentence_tokens" : [ "Applying", "to", "our", "data", ",", "with", "FFR", "measurement", "for", "AMI", "patients", "with", "borderline", "QFR", "zones", ",", "the", "overall", "classification", "agreement", "of", "the", "proposed", "QFR-FFR", "hybrid", "approach", "is", "94.1", "%", "." ]
}, {
  "sentence_tokens" : [ "QFR", "uses", "the", "3D", "rendering", "image", "once", "processed", "from", "angiography", "images", "and", "then", "performs", "computational", "calculations", "again", "." ]
}, {
  "sentence_tokens" : [ "Indirect", "diagnostic", "equipment", "with", "multistage", "calibration", "should", "be", "used", "in", "a", "manner", "that", "minimizes", "the", "risk", "of", "error.Another", "strength", "of", "this", "study", "was", "that", "we", "assessed", "the", "anatomical", "complexities", "of", "stenosis", "lesions", "to", "identify", "specific", "subsets", "with", "lower", "diagnostic", "accuracy", "." ]
}, {
