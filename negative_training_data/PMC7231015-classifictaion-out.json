}, {
  "sentence_tokens" : [ "These", "results", "indicate", "that", ",", "alongside", "liver", "function", "and", "tumor", "factors", ",", "immuno-nutritional", "status", "may", "be", "an", "important", "factor", "in", "the", "management", "of", "patients", "with", "HCC", "treatment", "with", "LEN.Treatment", "with", "LEN", "is", "a", "recommended", "therapy", "for", "unresectable", "HCC", ",", "according", "to", "the", "clinical", "guidelines", "for", "HCC", "in", "the", "United", "States", ",", "Europe", ",", "and", "Japan", "[", "3,5,6,32", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "our", "study", ",", "the", "MST", "was", "17.6", "months", "in", "patients", "with", "HCC", "treated", "with", "LEN", "." ]
}, {
  "sentence_tokens" : [ "Furthermore", ",", "in", "our", "study", ",", "the", "ORR", "and", "DCR", "were", "33", "%", "and", "74", "%", ",", "respectively", "." ]
}, {
  "sentence_tokens" : [ "Yamashita", "et", "al.", "previously", "reported", "an", "MST", "of", "17.6", "months", "in", "Japanese", "patients", "with", "unresectable", "HCC", "treated", "with", "LEN", "in", "a", "phase", "3", ",", "multinational", ",", "randomized", ",", "non", "inferiority", "trial", "(", "REFLECT", ")", "[", "33", "]", "." ]
}, {
  "sentence_tokens" : [ "Thus", ",", "the", "therapeutic", "effect", "of", "LEN", "in", "our", "study", "appears", "to", "be", "similar", "to", "that", "reported", "previously", "[", "7,11,33", "]", ",", "suggesting", "that", "the", "enrolled", "subjects", "and", "treatment", "effects", "in", "our", "study", "are", "typical.In", "this", "study", ",", "a", "CONUT", "score", "<", "5", ",", "ALBI", "grade", "1", ",", "BCLC", "stage", "B", ",", "and", "AFP", "<=", "200", "ng/mL", "were", "identified", "as", "independent", "factors", "for", "OS", ",", "using", "multivariate", "analysis", "." ]
}, {
  "sentence_tokens" : [ "Previous", "studies", "have", "reported", "that", "ALBI", "grade", ",", "AFP", "level", ",", "and", "BCLC", "stage", "are", "independent", "factors", "for", "OS", "in", "patients", "with", "HCC", "treated", "with", "LEN", "[", "7,9", "]", ",", "indicating", "that", "our", "results", "showed", "good", "agreement", "with", "those", "of", "previous", "reports", "." ]
}, {
  "sentence_tokens" : [ "The", "CONUT", "score", "has", "previously", "been", "reported", "as", "a", "prognostic", "factor", "for", "patients", "with", "HCC", "treated", "with", "hepatic", "resection", "[", "24,25,34", "]", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "we", "are", "the", "first", "to", "reveal", "that", "the", "CONUT", "score", "is", "an", "independent", "factor", "for", "OS", "in", "patients", "with", "HCC", "treated", "with", "LEN", "." ]
}, {
  "sentence_tokens" : [ "This", "indicates", "that", "the", "CONUT", "score", "is", "a", "prognostic", "factor", "independent", "from", "liver", "function", "and", "tumor", "factors", "." ]
}, {
  "sentence_tokens" : [ "Various", "factors", "are", "involved", "in", "the", "prognoses", "of", "patients", "with", "HCC", "treated", "with", "LEN", ";", "however", ",", "the", "prognostic", "importance", "of", "each", "factor", "has", "been", "unclear", "until", "now.In", "this", "study", ",", "we", "investigated", "the", "prognostic", "importance", "of", "each", "factor", "for", "OS", ",", "using", "decision", "tree", "analysis", "." ]
}, {
  "sentence_tokens" : [ "We", "found", "that", "the", "CONUT", "score", "was", "the", "initial", "splitting", "variable", "for", "survival", "rate", "in", "patients", "with", "HCC", "treated", "with", "LEN", ",", "followed", "by", "BCLC", "stage", "and", "ALBI", "grade", "." ]
}, {
  "sentence_tokens" : [ "Thus", ",", "the", "CONUT", "score", "may", "be", "the", "most", "important", "prognostic", "factor", "in", "patients", "treated", "with", "LEN", "." ]
}, {
  "sentence_tokens" : [ "Daitoku", "et", "al.", "report", "that", "the", "CONUT", "score", "is", "a", "prognostic", "factor", "in", "patients", "with", "metastatic", "colorectal", "cancer", "treated", "with", "chemotherapy", "[", "35", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "fact", ",", "the", "OS", "rate", "of", "the", "low", "CONUT", "score", "group", "was", "significantly", "higher", "than", "that", "of", "the", "high", "CONUT", "score", "group", "in", "our", "study", "." ]
}, {
  "sentence_tokens" : [ "It", "is", "unclear", "why", "the", "CONUT", "score", "was", "the", "most", "important", "predictive", "factor", "for", "OS", "in", "patients", "with", "HCC", "treated", "with", "LEN", "." ]
}, {
  "sentence_tokens" : [ "In", "fact", ",", "we", "performed", "the", "decision", "tree", "analysis", "with", "three", "parameters", "comprising", "the", "CONUT", "score", "separately", "." ]
}, {
  "sentence_tokens" : [ "In", "the", "analysis", ",", "the", "BCLC", "stage", "was", "identified", "as", "the", "variable", "for", "the", "initial", "split", "of", "survival", "(", "data", "not", "shown", ")", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "none", "of", "three", "parameters", "comprising", "the", "CONUT", "score", "was", "identified", "as", "the", "initial", "split", "of", "survival", ",", "suggesting", "that", "the", "combined", "power", "of", "the", "CONUT", "score", "was", "important", "for", "overall", "survival", "in", "patients", "treated", "with", "LEN.The", "CONUT", "score", "is", "based", "on", "total", "lymphocyte", "count", ",", "total", "cholesterol", "level", ",", "and", "serum", "albumin", "level", "." ]
}, {
  "sentence_tokens" : [ "All", "three", "of", "these", "factors", "showed", "significant", "correlations", "with", "total", "CONUT", "score", "in", "this", "study", "." ]
}, {
  "sentence_tokens" : [ "All", "three", "are", "also", "well-known", "nutritional", "parameters", "." ]
}, {
  "sentence_tokens" : [ "In", "fact", ",", "the", "total", "lymphocyte", "count", "is", "correlated", "with", "anthropometric", "nutritional", "indicators", ",", "such", "as", "mid", "upper", "arm", "circumference", "and", "triceps", "skinfold", "thickness", "[", "40", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "addition", ",", "the", "total", "lymphocyte", "count", "is", "utilized", "as", "an", "immune", "marker", ",", "and", "is", "known", "to", "correlate", "with", "the", "CD4+", "lymphocyte", "count", "[", "41,42", "]", "." ]
}, {
  "sentence_tokens" : [ "Moreover", ",", "cholesterol", "localizes", "in", "lipid", "rafts", "of", "the", "cell", "membrane", "." ]
}, {
  "sentence_tokens" : [ "Furthermore", ",", "albumin", "is", "the", "most", "abundant", "protein", "in", "plasma", ",", "and", "is", "known", "to", "act", "as", "an", "antioxidant", "via", "radical", "scavenging", "[", "44", "]", "." ]
}, {
  "sentence_tokens" : [ "This", "suggests", "that", "the", "CONUT", "score", "may", "reflect", "not", "only", "nutritional", "status", ",", "but", "also", "immunoresponsiveness", ",", "cell", "membrane", "lipid", "raft", "status", ",", "and", "antioxidative", "capacity", "." ]
}, {
  "sentence_tokens" : [ "This", "may", "explain", "why", "the", "CONUT", "score", "has", "been", "identified", "in", "this", "study", "as", "the", "most", "important", "variable", "associated", "with", "prognosis", "in", "patients", "with", "HCC", "treated", "with", "LEN.The", "cessation", "of", "treatment", "due", "to", "severe", "AE", "was", "significantly", "more", "frequent", "in", "the", "low", "CONUT", "group", "than", "in", "the", "high", "CONUT", "group", "in", "our", "study", "." ]
}, {
  "sentence_tokens" : [ "This", "suggests", "that", "a", "lower", "CONUT", "score", "may", "be", "associated", "with", "better", "OS", "through", "the", "longer", "duration", "of", "treatment", "with", "LEN", "." ]
}, {
  "sentence_tokens" : [ "We", "examined", "the", "impact", "of", "the", "parameters", "of", "the", "CONUT", "on", "discontinuation", "of", "LEN", "due", "to", "AE", "." ]
}, {
  "sentence_tokens" : [ "We", "found", "that", "serum", "albumin", "level", ",", "but", "not", "total", "lymphocyte", "count", "or", "total", "cholesterol", "level", ",", "was", "significantly", "higher", "in", "the", "no", "discontinuation", "due", "to", "AE", "group", "than", "in", "the", "discontinuation", "due", "to", "AE", "group", "." ]
}, {
  "sentence_tokens" : [ "Since", "lenvatinib", "is", "known", "to", "mainly", "bind", "to", "human", "serum", "albumin", "[", "45", "]", ",", "a", "higher", "serum", "albumin", "level", "may", "contribute", "to", "the", "suppression", "of", "the", "development", "of", "AE.This", "study", "could", "inform", "the", "experimental", "design", "of", "future", "prospective", "trials", "in", "this", "clinical", "context", "." ]
}, {
  "sentence_tokens" : [ "Our", "results", "may", "indicate", "the", "importance", "of", "evaluation", "for", "nutritional", "state", "of", "patients", ",", "using", "CONUT", "as", "a", "base", "line", "and", "throughout", "the", "study", "." ]
}, {
  "sentence_tokens" : [ "In", "addition", ",", "cholesterol", "is", "a", "precursor", "for", "bile", "acids", "and", "various", "other", "bioactive", "molecules", ",", "such", "as", "oxysterols", "[", "46", "]", ",", "which", "could", "have", "an", "impact", "on", "LEN", "uptake", "and", "action", "." ]
}, {
  "sentence_tokens" : [ "Thus", ",", "deeper", "probing", "of", "other", "metabolic", "parameters", ",", "including", "bile", "acids", "and", "immune", "status", ",", "is", "also", "required", "in", "future", "prospective", "trials.This", "study", "has", "several", "limitations", "." ]
}, {
  "sentence_tokens" : [ "First", ",", "the", "study", "was", "retrospective", "." ]
}, {
  "sentence_tokens" : [ "Second", ",", "we", "did", "not", "evaluate", "patient", "histories", "of", "previous", "and", "subsequent", "treatment", "for", "HCC", "." ]
}, {
  "sentence_tokens" : [ "Third", ",", "we", "did", "not", "evaluate", "sarcopenia", ",", "which", "has", "a", "significant", "relationship", "with", "both", "nutrition", "and", "prognosis", "." ]
}, {
  "sentence_tokens" : [ "To", "prove", "a", "prognostic", "impact", "of", "the", "CONUT", "score", "in", "patients", "with", "HCC", "treated", "with", "LEN", ",", "it", "would", "be", "necessary", "to", "perform", "a", "prospective", "study", ",", "including", "analysis", "of", "various", "host", "and", "tumor", "factors.In", "this", "study", ",", "we", "showed", "that", "a", "CONUT", "score", "<", "5", ",", "ALBI", "grade", "1", ",", "BCLC", "stage", "B", ",", "and", "AFP", "<=", "200", "ng/mL", "were", "independently", "associated", "with", "better", "prognosis", "in", "patients", "with", "HCC", "treatment", "with", "LEN", "." ]
}, {
  "sentence_tokens" : [ "Moreover", ",", "we", "revealed", "that", "the", "CONUT", "score", "was", "the", "most", "important", "variable", "for", "OS", "." ]
}, {
