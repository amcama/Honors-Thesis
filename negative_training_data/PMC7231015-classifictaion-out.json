}, {
  "sentence_tokens" : [ "Discussion", "." ]
}, {
  "sentence_tokens" : [ "Through", "this", "study", ",", "we", "demonstrated", "that", "a", "CONUT", "score", "<", "5", ",", "ALBI", "grade", "1", ",", "BCLC", "stage", "B", ",", "and", "AFP", "<=", "200", "ng/mL", "were", "independently", "associated", "with", "OS", "in", "patients", "with", "HCC", "treated", "with", "LEN", "." ]
