  }, {
    "sentence_tokens" : [ "Therefore", ",", "a", "relationship", "between", "CVCs", "or", "Hickman-catheters", "and", "the", "presence", "of", "T.", "pulmonis", "bacteremia", "exists", ";", "however", ",", "the", "risk", "of", "T.", "pulmonis", "bacteremia", "has", "not", "been", "reported", "with", "PICCs", "." ]
