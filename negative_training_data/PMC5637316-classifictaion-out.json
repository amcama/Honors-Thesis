  }, {
    "sentence_tokens" : [ "Although", "T.", "pulmonis", "generally", "causes", "CLABSI", "from", "CVCs", "and", "Hickman", "catheters", ",", "we", "report", "a", "case", "of", "PICC", "related", "bloodstream", "infection", "due", "to", "this", "microorganism", "and", "review", "the", "related", "literature.Table", "2", "summarizes", "the", "seven", "reported", "cases", "of", "T.", "pulmonis", "bacteremia", "in", "the", "literature", "[", "2", ",", "7", ",", "11", "-", "14", "]", "." ]
  }, {
    "sentence_tokens" : [ "Three", "patients", "had", "severe", "immunodeficiency", "combined", "with", "bone", "marrow", "transplantation", ",", "IgA", "nephropathy", ",", "or", "metastatic", "breast", "cancer", "as", "the", "underlying", "disease", "[", "7", "]", "." ]
