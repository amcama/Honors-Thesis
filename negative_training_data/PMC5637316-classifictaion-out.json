  }, {
    "sentence_tokens" : [ "In", "the", "present", "case", ",", "minimum", "inhibitory", "concentration", "(", "MIC", ")", "results", "using", "the", "broth", "dilution", "method", "showed", "susceptibility", "to", "vancomycin", "and", "cefozopran", "hydrochloride", "(", "Table", "1", ")", "." ]
  }, {
