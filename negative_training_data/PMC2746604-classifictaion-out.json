}, {
  "sentence_tokens" : [ "Therapeutic", "macromolecules", "must", "diffuse", "through", "the", "ECM", ",", "consisting", "of", "a", "fibrillar", "protein", "network", "embedded", "in", "a", "hydrophilic", "gel", "of", "GAG", ",", "in", "order", "to", "reach", "the", "tumour", "cells", "." ]
}, {
