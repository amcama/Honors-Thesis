}, {
  "sentence_tokens" : [ "In", "spheroids", "the", "cell", "density", "was", "rather", "heterogeneous", "with", "large", "areas", "without", "cells", ",", "whereas", "the", "tumours", "showed", "a", "more", "homogeneous", "cell", "density", "distribution", "." ]
}, {
  "sentence_tokens" : [ "This", "was", "observed", "for", "all", "three", "clones", "." ]
