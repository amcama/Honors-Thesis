}, {
  "sentence_tokens" : [ "Similarly", ",", "when", "investigating", "the", "effect", "of", "the", "food", "derived", "catalysts", "(", "polysulfides", ",", "isothiocyanates", ",", "carbon", "disulfide", ",", "and", "iodine", ")", "on", "Z", "isomerization", ",", "they", "were", "added", "to", "the", "mixture", "of", "tomato", "puree", ",", "i.e", ".,", "60:35:5", "(", "w/w/w", ")", "at", "a", "mixing", "ratio", "of", "tomato", "puree", ",", "distilled", "water", ",", "and", "olive", "oil", ",", "to", "a", "final", "concentration", "of", "0.01", ",", "0.1", ",", "1", ",", "and", "10", "mM", ",", "and", "then", "the", "thermal", "Z", "isomerization", "was", "conducted", "using", "the", "same", "condition", "as", "above", "." ]
}, {
