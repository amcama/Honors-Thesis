}, {
  "sentence_tokens" : [ "The", "above", "method", "was", "also", "applied", "to", "the", "commercially", "available", "raw", "tomatoes", "and", "tomato", "processed", "foods", "such", "as", "tomato", "juice", ",", "tomato", "puree", ",", "tomato", "ketchup", ",", "pizza", "sauce", ",", "and", "tomato", "soup", "." ]
}, {
  "sentence_tokens" : [ "HPLC", "analysis", "." ]
}, {
  "sentence_tokens" : [ "Normal-phase", "HPLC", "analysis", "of", "lycopene", "isomers", "was", "conducted", "according", "to", "previously", "described", "conditions", "." ]
}, {
