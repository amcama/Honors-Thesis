}, {
  "sentence_tokens" : [ "Extraction", "of", "lycopene", "isomers", "." ]
}, {
  "sentence_tokens" : [ "The", "extraction", "of", "lycopene", "isomers", "from", "the", "samples", "was", "carried", "out", "using", "acetone", ",", "according", "to", "a", "previously", "described", "method", "." ]
}, {
  "sentence_tokens" : [ "Unless", "specifically", "mentioned", ",", "all", "procedures", "were", "conducted", "at", "room", "temperature", ",", "and", "light", "exposure", "was", "kept", "to", "a", "minimum", "throughout", "the", "extraction", "." ]
