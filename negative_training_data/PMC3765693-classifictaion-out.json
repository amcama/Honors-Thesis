}, {
  "sentence_tokens" : [ "The", "latter", "makes", "it", "difficult", "to", "demonstrate", "a", "clinical", "benefit", "of", "genotyping", "in", "this", "patient", "population.Our", "data", "add", "to", "the", "evidence", "from", "clinical", "trial", "populations", "indicating", "an", "increased", "risk", "in", "carriers", "of", "the", "CYP2C19", "*", "2", "loss-of-function", "allele", "compared", "to", "non", "carriers", "taking", "clopidogrel", "[", "3,6,11", "]", "." ]
}, {
  "sentence_tokens" : [ "Although", "the", "prevalence", "of", "the", "homozygous", "carriers", "was", "low", "in", "our", "populations", "and", "the", "clinical", "relevance", "for", "the", "overall", "population", "of", "patients", "with", "stable", "CHD", "may", "be", "limited", ",", "the", "associated", "risk", "with", "a", "secondary", "CVD", "event", "for", "the", "few", "affected", "patients", "may", "be", "high", "." ]
