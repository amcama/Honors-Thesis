}, {
  "sentence_tokens" : [ "However", ",", "the", "internal", "validity", "of", "our", "findings", "should", "not", "be", "affected", "." ]
}, {
  "sentence_tokens" : [ "In", "addition", ",", "we", "considered", "clopidogrel", "use", "at", "baseline", "and", "one", "year", "follow-up", ",", "but", "the", "low", "prevalence", "probably", "reflects", "clinical", "practice", "a", "decade", "ago", "." ]
}, {
  "sentence_tokens" : [ "Conclusions", "." ]
}, {
