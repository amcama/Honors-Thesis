}, {
  "sentence_tokens" : [ "This", "is", "in", "line", "with", "the", "results", "of", "a", "meta-analysis", "of", "Holmes", "and", "colleagues", "[", "12", "]", "who", "compared", "individuals", "with", "any", "copy", "of", "CYP2C19", "genetic", "variants", "(", "alleles", "*", "2", "through", "*", "8)", ";", "presumably", ",", "the", "vast", "majority", "of", "subjects", "carried", "a", "heterozygous", "genotype", "." ]
}, {
  "sentence_tokens" : [ "Although", "they", "found", "a", "higher", "risk", "for", "CVD", "events", "of", "1.18", "(", "95", "%", "CI", "1.09-1.28", ")", "using", "fixed-effects", "models", "and", "of", "1.34", "(", "95", "%", "CI", "1.15-1.56", ")", "for", "a", "random", "effects", "model", ",", "there", "was", "a", "trend", "towards", "the", "null", "in", "larger", "studies", "consistent", "with", "small", "study", "bias", "and", "they", "concluded", "that", "there", "was", "no", "association", "of", "genotype", "with", "CVD", "events", "." ]
