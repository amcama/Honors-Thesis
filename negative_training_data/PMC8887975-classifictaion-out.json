  "sentence_tokens" : [ "In", "recent", "years", ",", "the", "transcutaneous", "bilirubinometer", ",", "which", "uses", "photometry", "to", "detect", "bilirubin", "levels", ",", "has", "been", "used", "as", "an", "alternative", "to", "estimate", "the", "bilirubin", "levels.A", "transcutaneous", "bilirubinometer", "is", "a", "portable", ",", "painless", "and", "non", "invasive", "device", "." ]
