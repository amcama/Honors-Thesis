}, {
  "sentence_tokens" : [ "The", "diagnostic", "accuracy", "of", "the", "TcB", "measured", "at", "the", "forehead", "and", "sternum", "at", "a", "TSB", "level", "of", "205", "mmol/L", "indicates", "that", "there", "are", "not", "much", "differences", "in", "term", "of", "sensitivity", ",", "specificity", ",", "positive", "as", "well", "as", "negative", "predictive", "values", "(", "Table", "3", ")", "." ]
}, {
  "sentence_tokens" : [ "Figure", "5", "illustrates", "the", "difference", "between", "TcB", "and", "TSB", "values", "throughout", "the", "measurement", "range", "of", "TSB", "." ]
