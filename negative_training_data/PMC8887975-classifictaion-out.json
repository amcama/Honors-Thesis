}, {
  "sentence_tokens" : [ "Since", "then", ",", "numerous", "studies", "using", "various", "transcutaneous", "bilirubinometer", "devices", "have", "been", "conducted", "to", "prove", "its", "accuracy", "and", "sensitivity", "." ]
