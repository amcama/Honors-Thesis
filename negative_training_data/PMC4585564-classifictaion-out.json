}, {
  "sentence_tokens" : [ "In", "addition", ",", "a", "significant", "amount", "of", "Glu", "molecules", "were", "observed", "on", "the", "MO/C", "surfaces", ",", "confirmed", "by", "a", "corresponding", "weight", "loss", "at", "200", "-", "300", "degrees", "C", "in", "thermogravimetric", "analysis", "(", "data", "not", "shown", ")", ",", "suggesting", "glucose", "combustion", "." ]
