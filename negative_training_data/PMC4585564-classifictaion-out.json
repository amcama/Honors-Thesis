  "sentence_tokens" : [ "2", "nm", "were", "observed", "(", "Fig.", "4a", ")", ",", "corresponding", "to", "the", "size", "of", "Pt", "NPs", "in", "Pt/C", ",", "while", "significant", "growth", "of", "Pt", "NP", "(", ">", "10", "nm", ")", "was", "observed", "in", "Pt/NaCl", "(", "Supplementary", "Fig.", "13", "online", ")", "." ]
}, {
  "sentence_tokens" : [ "From", "the", "X-ray", "photoelectron", "spectroscopy", "(", "XPS", ")", "curves", "of", "Pt/Glu", "and", "Pt/NaCl", ",", "the", "Pt", "-", "OH", "moiety", "was", "more", "abundant", "in", "Pt/Glu", ",", "indicating", "significant", "interactions", "between", "Pt", "and", "Glu", "." ]
}, {
  "sentence_tokens" : [ "In", "contrast", ",", "comparable", "Pt", "4f", "XPS", "curves", "were", "obtained", "for", "Pt/NaCl", "and", "commercial", "Pt/C", ",", "suggesting", "little", "interactions", "between", "Pt", "NPs", "and", "NaCl", "(", "Fig.", "4b", ",", "c", ",", "and", "Supplementary", "Fig.", "14", "online", ")", "." ]
}, {
  "sentence_tokens" : [ "As", "Pt/Glu", "and", "Pt/NaCl", "were", "prepared", "according", "to", "the", "same", "method", ",", "the", "-", "OH", "moieties", "were", "considered", "to", "originate", "from", "Glu", "." ]
}, {
  "sentence_tokens" : [ "The", "surface", "concentrations", "of", "Pt", "in", "Pt/Glu", "and", "Pt/NaCl", "were", "virtually", "identical", ",", "and", "thus", "the", "Glu", "substrate", "was", "expected", "to", "stabilize", "the", "Pt", "NPs", "." ]
}, {
