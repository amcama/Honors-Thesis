}, {
  "sentence_tokens" : [ "The", "authors", "identified", "a", "novel", "keloid", "biomarker", ",", "Tenascin-c", "(", "TNC", ")", ",", "by", "single-cell", "analysis", ",", "weighted", "co-expression", "network", "analysis", ",", "and", "differential", "gene", "analysis", ",", "and", "verified", "TNC", "expression", "by", "RNA", "sequencing", "in", "clinical", "keloid", "samples", "." ]
