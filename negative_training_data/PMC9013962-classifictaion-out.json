}, {
  "sentence_tokens" : [ "Keloids", ",", "a", "common", "and", "frequently", "occurring", "condition", "in", "plastic", "surgery", "and", "dermatology", ",", "show", "tumor", "like", "characteristics", "such", "as", "treatment", "resistance", "and", "a", "high", "recurrence", "rate", "(", "1", ")", "." ]
}, {
  "sentence_tokens" : [ "Keloids", "are", "a", "product", "of", "abnormal", "healing", "after", "skin", "trauma", "and", "often", "cause", "symptoms", "such", "as", "itching", ",", "pain", ",", "and", "local", "paresthesia", "within", "the", "growth", "range", "(", "2", ",", "3", ")", "." ]
