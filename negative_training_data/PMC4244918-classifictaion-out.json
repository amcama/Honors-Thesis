  "sentence_tokens" : [ "First", ",", "although", "seven", "studies", "were", "all", "randomized", "controlled", "trials", ",", "confounding", "factors", "such", "as", "tumor", "location", ",", "tumor", "stage", ",", "the", "use", "of", "diverting", "ileostomy", ",", "the", "anastomotic", "integrity", "using", "endoscopy", "or", "enema", "with", "contrast", "medium", ",", "and", "the", "definition", "of", "AL", "inevitably", "existed", "." ]
