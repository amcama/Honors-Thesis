}, {
  "sentence_tokens" : [ "Introduction", "." ]
}, {
  "sentence_tokens" : [ "Glioblastoma", "(", "GBM", ")", "is", "one", "of", "the", "most", "malignant", "brain", "tumors", "." ]
}, {
  "sentence_tokens" : [ "After", "the", "postoperative", "use", "of", "radiotherapy", "for", "GBM", "became", "common", ",", "a", "phenomenon", "termed", "pseudoprogression", "disease", "(", "PsPD", ")", "was", "identified", "[", "1,2", "]", "." ]
}, {
  "sentence_tokens" : [ "With", "the", "widely", "implementation", "of", "the", "Stupp", "protocol", "for", "treating", "GBM", ",", "this", "phenomenon", "has", "been", "inceasingly", "reported", ",", "with", "an", "incidence", "rate", "varies", "among", "reports", "(", "5.5", "%-64%", ")", "[", "3-6", "]", "." ]
}, {
  "sentence_tokens" : [ "PsPD", "is", "often", "misdiagnosed", "as", "tumor", "recurrence", "and", "misleads", "the", "clinical", "treatment", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "little", "is", "known", "about", "why", "PsPD", "occurs", "in", "a", "subset", "of", "GBM", "patients", "and", "the", "fundamental", "biological", "features", "of", "PsPD", "remain", "unclear", "[", "5,7-10]", "." ]
}, {
  "sentence_tokens" : [ "From", "a", "diagnostic", "perspective", ",", "no", "single", "imaging", "technique", ",", "including", "T1", "weighted", "magnetic", "resonance", "imaging", "(", "MRI", ")", ",", "magnetic", "resonance", "spectroscopy", "(", "MRS", ")", ",", "relative", "cerebral", "blood", "volume", "(", "rCBV", ")-based", "parametric", "response", "mapping", "and", "fluorodeoxyglucose", "(", "F-FDG", ")-positron", "emission", "computed", "tomography", "(", "PET", ")", ",", "has", "been", "adequate", "for", "differentiating", "PsPD", "from", "true", "early", "tumor", "progression", "with", "high", "sensitivity", "and", "specificity", "[", "4,5,11-16]", "." ]
}, {
  "sentence_tokens" : [ "Moreover", ",", "molecular", "biological", "studies", "have", "failed", "to", "uncover", "biomarkers", "linked", "to", "PsPD", "for", "clinical", "use", "." ]
}, {
  "sentence_tokens" : [ "Although", "a", "multitude", "of", "genetic", "and", "molecular", "changes", "involved", "in", "GBM", ",", "including", "O6-methylguanine", "-", "DNA", "methyltransferase", "(", "MGMT", ")", "promoter", "methylation", ",", "isocitrate", "dehydrogenase", "1", "(", "IDH1", ")", "mutation", ",", "p53", "mutation", "and", "Ki-67", "expression", ",", "have", "been", "found", "to", "be", "associated", "with", "PsPD", ",", "the", "predictive", "value", "of", "these", "biomarkers", "remains", "debatable", "[", "5,8,17-19]", "." ]
}, {
  "sentence_tokens" : [ "Therefore", ",", "except", "for", "cases", "of", "pathological", "verification", ",", "PsPD", "is", "still", "predominantly", "diagnosed", "retrospectively", "." ]
