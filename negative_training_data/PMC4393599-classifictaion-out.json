  "sentence_tokens" : [ "These", "findings", "indicate", "a", "hypothesis", "that", "an", "underlying", "relationship", "between", "PsPD", "occurrence", "and", "cell", "death", "induction", "by", "adjuvant", "therapy", "may", "exist", "[", "30", "]", "." ]
}, {
