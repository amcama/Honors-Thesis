  "sentence_tokens" : [ "Thus", ",", "there", "is", "an", "urgent", "need", "for", "the", "exploration", "of", "more", "reliable", "biochemical", "markers", "that", "can", "accurately", "identify", "PsPD.Proteomic", "measurements", "provide", "a", "wealth", "of", "biological", "information", "and", "several", "proteomic", "studies", "of", "gliomas", "have", "been", "recently", "reported", "[", "20,21", "]", ",", "which", "demonstrated", "a", "possibility", "to", "investigate", "this", "phenomenon", "by", "using", "proteomics", "methods", "." ]
}, {
  "sentence_tokens" : [ "Herein", ",", "this", "present", "study", "was", "designed", "to", "identify", "biological", "signatures", "and", "explore", "biomarkers", "for", "PsPD", "using", "differential", "proteomic", "techniques", "(", "Figure", "1", ")", "." ]
