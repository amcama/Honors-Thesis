}, {
  "sentence_tokens" : [ "Scaffold", "software", "was", "used", "to", "further", "filter", "the", "database", "search", "results", "using", "the", "decoy", "database", "method", "with", "the", "following", "filter", ":", "a", "1", "%", "false", "positive", "rate", "at", "the", "protein", "level", "and", "two", "unique", "peptides", "per", "protein", "." ]
}, {
  "sentence_tokens" : [ "After", "filtering", "the", "results", "as", "described", "above", ",", "the", "peptide", "abundances", "in", "different", "reporter", "ion", "channels", "of", "the", "MS/MS", "scan", "were", "normalized", "." ]
}, {
  "sentence_tokens" : [ "The", "protein", "abundance", "ratio", "was", "based", "on", "unique", "peptide", "results", "." ]
}, {
  "sentence_tokens" : [ "Proteins", "with", "a", "fold", "change", ">=", "2", "were", "considered", "significantly", "altered", "." ]
}, {
  "sentence_tokens" : [ "Bioinformatics", "analysis", "." ]
}, {
  "sentence_tokens" : [ "Data", "filtering", "was", "performed", "according", "to", "strict", "criteria", ",", "wherein", "any", "missing", "data", "values", "or", "detection", "failures", "were", "deleted", "." ]
}, {
  "sentence_tokens" : [ "Pearson", "'", "s", "correlation", "coefficient", "was", "calculated", "to", "measure", "the", "quantitative", "correlation", "among", "the", "three", "biological", "replicates", "in", "each", "group", ",", "and", "the", "coefficient", "of", "variation", "within", "groups", "was", "set", "at", "CV", "=", "0.37", "to", "filter", "out", "low-quality", "data", "." ]
}, {
  "sentence_tokens" : [ "A", "Student", "'", "s", "t-test", "was", "performed", "between", "groups", ",", "and", "differences", "were", "considered", "to", "be", "significant", "when", "p", "<", "0.05", "." ]
}, {
  "sentence_tokens" : [ "Any", "proteins", "that", "satisfied", "the", "criteria", "of", "a", "fold", "change", "(", "FC", ")", "between", "groups", "of", ">=", "2", "were", "selected", "for", "bioinformatics", "analysis", "using", "Gene", "Ontology", "(", "GO", ")", "and", "Ingenuity", "Pathway", "Analysis", "(", "IPA", ")", "." ]
}, {
  "sentence_tokens" : [ "GO", "functional", "and", "IPA", "network", "analysis", "." ]
}, {
  "sentence_tokens" : [ "All", "proteins", "identified", "by", "the", "two", "approaches", "were", "assigned", "a", "gene", "symbol", "using", "the", "Panther", "database", "(", "http://www.pantherdb.org/", ")", "." ]
}, {
  "sentence_tokens" : [ "Protein", "classification", "was", "performed", "based", "on", "the", "functional", "annotations", "of", "the", "GO", "project", "for", "cellular", "compartment", ",", "molecular", "functional", "and", "biological", "processed", "." ]
}, {
  "sentence_tokens" : [ "When", "more", "than", "one", "assignment", "was", "available", ",", "all", "of", "the", "functional", "annotations", "were", "considered", "in", "the", "results", "." ]
}, {
  "sentence_tokens" : [ "Moreover", ",", "all", "of", "the", "selected", "proteins", "with", "a", "significant", "fold", "changes", "were", "used", "for", "pathway", "analysis", "using", "the", "IPA", "software", "(", "Ingenuity", "Systems", ",", "Mountain", "View", ",", "CA", ")", "for", "network", "analysis", "." ]
}, {
  "sentence_tokens" : [ "Immunohistochemistry", "and", "western", "blot", "analysis", "." ]
}, {
  "sentence_tokens" : [ "IHC", "was", "performed", "on", "the", "same", "six", "tissue", "samples", "used", "for", "the", "proteomic", "analysis", "and", "on", "twelve", "additional", "formalin", "fixed", ",", "paraffin", "embedded", "tissue", "samples", "." ]
}, {
  "sentence_tokens" : [ "The", "following", "primary", "antibodies", "were", "used", ":", "anti-ELAVL1mouse", "monoclonal", "(", "Santa", "Cruz", ")", ",", "1:500", ";", "anti-HNRNPK", "mouse", "monoclonal", "(", "Santa", "Cruz", ")", ",", "1:50", ";", "anti-CDH2rabbit", "monoclonal", "(", "Cell", "Signaling", "Technology", ")", ",", "1:250", ";", "anti-FBLN1", "mouse", "monoclonal", "(", "Santa", "Cruz", ")", ",1:125", ";", "anti-CALU", "goat", "polyclonal", "(", "Santa", "Cruz", ")", ",", "1:100", ";", "anti-FGB", "goat", "polyclonal", "(", "Abcam", ")", ",", "1:16000", "." ]
}, {
  "sentence_tokens" : [ "After", "deparaffinization", "and", "rehydration", ",", "antigen", "retrieval", "was", "performed", "by", "immersing", "the", "slide", "in", "antigenretrieval", "buffer", "(", "10", "mM", "sodium", "citrate", ",", "0.05", "%", "Tween", "20", ",", "pH", "=", "6.0", ")", "at", "95", "degrees", "C", "for", "5", "min", "using", "pressure", "cooker", "." ]
}, {
  "sentence_tokens" : [ "Endogenous", "peroxidases", "were", "blocked", "with", "0.03", "%", "hydrogen", "peroxide", ",", "and", "nonspecific", "binding", "was", "blocked", "with", "2", "%", "fetal", "calf", "serum", "in", "Tris", "buffered", "saline", "with", "0.1", "%", "Triton", "X-100", "(", "TBST", ",", "pH", "=", "7.6", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "sections", "were", "then", "incubated", "for", "1", "h", "at", "room", "temperature", "with", "primary", "antibodies", "followed", "by", "peroxidase", "labeled", "polymer", "conjugate", "to", "anti-mouse", ",", "anti-rabbit", ",", "anti-goat", "immunoglobulins", "for", "1", "h", "and", "developed", "with", "diaminobenzidine", "system", "." ]
}, {
  "sentence_tokens" : [ "The", "sections", "were", "counter", "stained", "with", "the", "Mayer", "'", "s", "hematoxylin", "and", "dehydrated", ",", "and", "the", "image", "was", "taken", "under", "microscope.WBs", "of", "the", "same", "six", "samples", "and", "additional", "twelve", "samples", "was", "performed", "to", "validate", "the", "proteomic", "quantitation", "of", "four", "selected", "candidate", "proteins", "(", "HNRNPK", ",", "ELAVL1", ",", "CDH2", "and", "FBLN1", ")", "." ]
}, {
  "sentence_tokens" : [ "Proteins", "extracted", "from", "GBM", "or", "PsPD", "tissues", "were", "resolved", "by", "SDS-PAGE", "(", "4", "-", "20", "%", "gradient", "precast", "gel", ";", "Invitrogen", ")", "." ]
}, {
