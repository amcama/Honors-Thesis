  "sentence_tokens" : [ "No", "paediatric", "study", "with", "a", "matched", "control", "group", "is", "available", "in", "literature", ",", "and", "our", "study", "'", "s", "aim", "is", "to", "fill", "this", "gap", "." ]
}, {
