}, {
  "sentence_tokens" : [ "Interestingly", ",", "Inaba", "et", "al.", "conducted", "a", "prospective", "analysis", "and", "compared", "the", "efficacy", "of", "small", "(", "28", "-", "32", "F", ")", "versus", "large", "(", "36", "-", "40", "F", ")", "thoracostomy", "tubes", "in", "293", "patients", "with", "thoracic", "traumas", "and", "concluded", "no", "differences", "in", "retained", "hemothoraces", ",", "the", "need", "for", "additional", "tube", "insertion", "and/or", "pain", "level", "(", "Inaba", "et", "al.", "2012", ")", "." ]
