}, {
  "sentence_tokens" : [ "In", "general", ",", "the", "use", "of", "a", "large", "bore", "32", "F", "or", "greater", "is", "likely", "to", "reduce", "the", "complication", "associated", "with", "a", "drain", "becoming", "kinked", "or", "clotted", "." ]
