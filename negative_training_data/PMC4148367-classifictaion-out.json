  "sentence_tokens" : [ "Correlation", "analysis", "." ]
}, {
  "sentence_tokens" : [ "Spearman", "'", "s", "correlation", "analysis", "demonstrated", "that", "the", "expression", "of", "p65", "in", "the", "epithelial", "nuclei", "of", "the", "NS", "and", "the", "CA", "and", "VV", "samples", "was", "negatively", "correlated", "with", "the", "HMGB1", "level", "in", "the", "epithelial", "cell", "nuclei", "(", "r", "=", "-", "0.1947", ",", "P", ">", "0.05", ";", "Fig.", "4A", "and", "G", ")", ",", "was", "positively", "correlated", "with", "p65", "in", "the", "inflammatory", "cell", "nuclei", "(", "r", "=0", ".0536", ",", "P", ">", "0.05", ";", "Fig.", "4B", "and", "4G", ")", ",", "was", "positively", "correlated", "with", "TLR4", "on", "the", "surface", "of", "the", "epithelial", "cell", "membranes", "(", "r", "=0", ".0949", ",", "P", ">", "0.05", ";", "Fig.", "4C", "and", "G", ")", ",", "with", "p65", "in", "the", "vascular", "endothelial", "cells", "(", "r", "=0", ".4355", ",", "P", "=0", ".0002", ";", "Fig.", "4D", "and", "G", ")", ",", "with", "HMGB1", "in", "the", "inflammatory", "cells", "(", "r", "=0", ".4879", ",", "P", "=0", ".0000", ";", "Fig.", "4E", "and", "G", ")", "and", "with", "HMGB1", "in", "the", "epithelial", "intercellular", "spaces", "(", "r", "=0", ".5199", ",", "P", "=0", ".0000", ";", "Fig.", "4F", "and", "G", ")", "." ]
}, {
  "sentence_tokens" : [ "Discussion", "." ]
}, {
  "sentence_tokens" : [ "CA", "and", "VV", "are", "the", "most", "common", "epidermal", "infection", "caused", "by", "HPVs", "." ]
}, {
