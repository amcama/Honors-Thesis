}, {
  "sentence_tokens" : [ "Instead", ",", "the", "impairment", "of", "TLR4", "may", "be", "a", "factor", "for", "the", "development", "of", "VV", "." ]
}, {
  "sentence_tokens" : [ "The", "present", "study", "also", "demonstrated", "that", "the", "TLR4", "expression", "on", "the", "surface", "of", "the", "epithelial", "membrane", "in", "CA", "samples", "was", "significantly", "higher", "than", "that", "of", "the", "NS", "(", "P", "<", "0.01", ")", ",", "and", "that", "the", "extracellular", "HMGB1", "expression", "in", "CA", "was", "significantly", "higher", "than", "in", "the", "NS", "(", "P", "<", "0.01", ")", "." ]
}, {
  "sentence_tokens" : [ "These", "findings", "indicate", "that", "extracellular", "HMGB1", "may", "act", "as", "a", "pro", "inflammatory", "factor", "to", "interact", "with", "TLR4", ",", "and", "that", "the", "HMGB1-TLR4", "signaling", "pathway", "associated", "inflammation", "may", "have", "a", "significant", "role", "in", "CA.The", "NF-kappa", "B", "family", "mainly", "includes", "RELA", "(", "p65", ")", ",", "NF-kappa", "B1", "(", "p50", ";", "p105", ")", ",", "NF-kappa", "B2", "(", "p52", ";", "p100", ")", ",", "c-REL", "and", "RELB", "(", "23,24", ")", "." ]
}, {
  "sentence_tokens" : [ "NF-kappa", "B", "is", "sequestered", "in", "the", "cytoplasm", "inactively", "and", "bound", "by", "the", "inhibitor", "proteins", "(", "24", ")", "." ]
}, {
  "sentence_tokens" : [ "Activated", "by", "various", "stimuli", ",", "including", "different", "pro", "inflammatory", "cytokines", ",", "NF-kappa", "B", "is", "transported", "to", "the", "nuclei", ",", "where", "it", "binds", "with", "sequences", "of", "various", "genes", "to", "induce", "the", "expression", "of", "inflammatory", "factors", ",", "promote", "cell", "proliferation", "and", "prevent", "apoptosis", ",", "and", "may", "therefore", "have", "a", "role", "in", "tumorigenesis", "and", "tumor", "development", "(", "25,26", ")", "." ]
}, {
  "sentence_tokens" : [ "A", "previous", "study", "demonstrated", "that", "NF-kappa", "B", "expression", "and", "activation", "are", "abnormal", "in", "human", "epidermal", "tumors", "of", "seborrheic", "keratosis", ",", "basal", "cell", "carcinoma", "and", "squamous", "cell", "carcinoma", "(", "11", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "present", "study", "revealed", "that", "there", "was", "weak", "positive", "expression", "of", "p65", "in", "the", "epithelial", "nuclei", "of", "VV", "and", "CA", "samples", ",", "while", "in", "the", "NS", "there", "was", "only", "minimal", "p65", "expression", "in", "the", "epithelial", "cell", "nuclei", "." ]
}, {
  "sentence_tokens" : [ "ANOVA", "demonstrated", "that", "the", "p65", "expression", "in", "the", "epithelial", "nuclei", "of", "VV", "and", "CA", "samples", "was", "significantly", "higher", "than", "in", "the", "NS", "(", "P", "<", "0.01", ")", "." ]
}, {
  "sentence_tokens" : [ "These", "findings", "indicate", "that", "p65", "is", "transported", "to", "the", "nuclei", "due", "to", "NF-kappa", "B", "activation", ",", "and", "thus", "induces", "the", "expression", "of", "inflammatory", "factors", ",", "promoting", "cell", "proliferation", "and", "preventing", "apoptosis", "in", "the", "warts", "." ]
}, {
  "sentence_tokens" : [ "Western", "blot", "analysis", "also", "demonstrated", "a", "significant", "p65", "expression", "in", "the", "epithelial", "nuclei", "of", "CA", "and", "VV", "samples", ",", "whereas", "only", "a", "weak", "p65", "expression", "in", "the", "NS", "was", "identified", "." ]
}, {
  "sentence_tokens" : [ "NF-kappa", "B", "is", "sequestered", "and", "bound", "by", "the", "IkB", "family", "of", "inhibitor", "proteins", "in", "the", "cytoplasm", "in", "its", "inactive", "form", "." ]
}, {
  "sentence_tokens" : [ "Various", "stimuli", ",", "including", "proinflammatory", "cytokines", "and", "viral", "replication", "are", "able", "to", "activate", "NF-kappa", "B", "by", "triggering", "the", "phosphorylation", "of", "IkB", ",", "and", "induce", "its", "subsequent", "molecular", "translocation", "to", "the", "nucleus", "." ]
}, {
  "sentence_tokens" : [ "As", "expected", ",", "the", "immunohistochemistry", "and", "western", "blotting", "results", "proved", "that", "the", "level", "of", "p65", "expression", "in", "the", "epithelial", "nuclei", ",", "which", "is", "indicative", "of", "NF-kappa", "B", "activation", "and", "inflammation", "responses", ",", "was", "increased", "and", "significantly", "higher", "in", "VV", "and", "CA", "than", "in", "the", "NS", "(", "P", "<", "0.01", ")", ",", "suggesting", "that", "inflammation", "may", "be", "intensified", "in", "the", "HPV", "infected", "epithelial", "cells", ",", "which", "may", "lead", "to", "the", "development", "of", "warts", "." ]
}, {
  "sentence_tokens" : [ "The", "expression", "of", "p65", "in", "the", "vascular", "endothelial", "cells", "in", "the", "VV", "and", "CA", "samples", "was", "significantly", "higher", "than", "in", "the", "NS", "(", "P", "<", "0.01", ")", "." ]
}, {
  "sentence_tokens" : [ "In", "addition", ",", "the", "p65", "level", "in", "the", "vascular", "endothelial", "cells", "was", "positively", "correlated", "with", "the", "p65", "level", "in", "the", "epithelial", "nuclei", "of", "the", "tissues", "(", "r", "=0", ".4355", ",", "P", "<", "0.01", ")", ",", "indicating", "that", "NF-kappa", "B", "may", "activate", "the", "transcription", "and", "translation", "of", "genes", "to", "promote", "angiogenesis", "in", "the", "warts.Furthermore", ",", "the", "expression", "of", "p65", "in", "the", "associated", "inflammatory", "cell", "nuclei", "in", "the", "VV", "samples", "was", "higher", "than", "the", "other", "tissues", "(", "P", "<", "0.05", ")", "." ]
}, {
