}, {
  "sentence_tokens" : [ "Next", ",", "membranes", "were", "washed", "three", "times", "with", "PBS-Tween", "and", "proteins", "were", "detected", "by", "chemiluminiscence", "using", "a", "ChemiDocTM", "XRS+", "System", "(", "Bio-Rad", ",", "Hercules", ",", "CA", ",", "USA", ")", "." ]
}, {
  "sentence_tokens" : [ "2.7", "." ]
}, {
  "sentence_tokens" : [ "Statistical", "Analyses", "." ]
}, {
  "sentence_tokens" : [ "To", "test", "the", "significance", "of", "the", "differences", ",", "one-way", "analysis", "of", "the", "variance", "(", "ANOVA", ")", "was", "performed", "using", "the", "statistical", "package", "SPSS", "15", "(", "SPSS", "Inc", ",", "Chicago", ",", "IL", ",", "USA", ")", ",", "applying", "Bonferroni", "'", "s", "correction", "for", "multiple", "comparisons", "." ]
