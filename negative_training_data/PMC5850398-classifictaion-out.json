  "sentence_tokens" : [ "Cells", "were", "lysed", "on", "ice", "in", "lysis", "buffer", "(", "150", "mM", "NaCl", ",", "5", "mM", "beta-mercaptoethanol", ",", "1", "%", "NP-40", ",", "0.1", "%", "sodium", "dodecyl", "sulfate", "(", "SDS", ")", ",", "50", "mM", "Tris-HCl", "pH", "8)", "supplemented", "with", "Benzonase", "Nuclease", "(", "Novagen", ",", "EMD", "Chemicals", ",", "San", "Diego", ",", "CA", ",", "USA", ")", "and", "cOmplete", "protease", "inhibitor", "cocktail", "tablets", "(", "Roche", ",", "Indiannapolis", ",", "IN", ",", "USA", ")", "." ]
