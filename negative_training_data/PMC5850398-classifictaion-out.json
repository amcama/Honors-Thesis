}, {
  "sentence_tokens" : [ "Inhibition", "of", "WNV", "Multiplication", "after", "Treatment", "with", "PKC", "Inhibitors", "." ]
}, {
  "sentence_tokens" : [ "Calphostin", "C", "and", "chelerythrine", ",", "broad-PKC", "inhibitors", "with", "different", "sites", "of", "interaction", "within", "the", "PKC", "protein", "structure", ",", "and", "staurosporine", "and", "enzastaurin", ",", "specific", "inhibitors", "for", "conventional", "and", "novel", "PKC", "isoforms", "[", "35,36", "]", ",", "were", "tested", "for", "their", "capability", "to", "inhibit", "WNV", "multiplication", "in", "Vero", "cells", "." ]
}, {
  "sentence_tokens" : [ "Calphostin", "C", ",", "with", "reported", "IC", "of", "50", "nM", "(", "https://pubchem.ncbi.nlm.nih.gov/compound/2533", ")", ",", "and", "chelerythrine", ",", "with", "IC", "of", "0.66", "mu", "M", "[", "37", "]", ",", "were", "used", "at", "200", "nM", "and", "8", "mu", "M", ",", "respectively", "." ]
}, {
  "sentence_tokens" : [ "Staurosporine", ",", "with", "IC", "of", "2,7", "nM", "[", "38", "]", "and", "enzastaurin", ",", "with", "IC", "of", "2", "-", "73", "nM", "[", "35", "]", ",", "were", "both", "used", "at", "200", "nM", "." ]
}, {
  "sentence_tokens" : [ "In", "order", "to", "determine", "the", "optimal", "point", "to", "analyze", "the", "effect", "of", "the", "drugs", "on", "WNV", "infection", ",", "cell", "viability", "was", "assayed", "at", "different", "time", "points", "from", "24", "to", "72", "h", "after", "drug", "treatment", "(", "Figure", "1a", ")", "and", "a", "viral", "growth", "curve", "was", "determined", "(", "Figure", "1b", ")", "." ]
}, {
  "sentence_tokens" : [ "Considering", "these", "results", ",", "samples", "were", "analyzed", "at", "24", "h", "p.i", ".,", "when", "viral", "yields", "were", "at", "the", "plateau", "(", "Figure", "1b", ")", "without", "drugs", "affecting", "cell", "viability", "(", "higher", "than", "80", "%", ")", "(", "Figure", "1a", ")", "." ]
}, {
  "sentence_tokens" : [ "At", "the", "tested", "concentrations", ",", "only", "calphostin", "C", "and", "chelerythrine", "significantly", "inhibited", "WNV", "multiplication", "(", "Figure", "1c", ")", "." ]
}, {
  "sentence_tokens" : [ "No", "effect", "was", "observed", "neither", "with", "staurosporine", "nor", "with", "enzastaurin", ",", "which", "inhibit", "both", "conventional", "and", "novel", "PKC", "isoforms", "(", "Figure", "1c", ")", "." ]
}, {
  "sentence_tokens" : [ "Detection", "of", "the", "WNV", "envelope", "(", "E", ")", "glycoprotein", "in", "virus", "infected", "cells", "treated", ",", "or", "not", ",", "with", "the", "drugs", "that", "exerted", "a", "previous", "inhibitory", "effect", "was", "performed", "by", "immunofluorescence", "and", "Western", "blot", ",", "confirming", "these", "results", "." ]
}, {
  "sentence_tokens" : [ "The", "intensity", "of", "fluorescence", "was", "significantly", "reduced", "in", "infected", "cells", "treated", "with", "either", "drug", "when", "compared", "to", "non", "treated", "control", "infected", "cells", "(", "Figure", "2a", ",", "b", ")", ",", "as", "was", "the", "WNV-E", "protein", "expected", "band", "observed", "by", "Western", "blot", "(", "Figure", "2c", ")", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "while", "quantitative", "RT-PCR", "analysis", "of", "the", "amount", "of", "viral", "RNA", "released", "to", "the", "supernatant", "24", "h", "p.i", "." ]
}, {
  "sentence_tokens" : [ "was", "significantly", "reduced", "in", "infected", "cells", "treated", "with", "chelerythrine", ",", "an", "almost", "negligible", "decrease", "was", "observed", "in", "calphostin", "C", "treated", "cells", "(", "Figure", "2d", ")", "." ]
}, {
  "sentence_tokens" : [ "Taken", "together", ",", "these", "results", "suggested", "that", "PKCs", ",", "mainly", "those", "of", "the", "atypical", "subfamily", ",", "are", "likely", "involved", "in", "WNV", "multiplication", ",", "even", "though", "differences", "in", "the", "specificity", "for", "PKC", "isoforms", "displayed", "by", "drugs", "produced", "differences", "in", "their", "inhibitory", "effect", "." ]
}, {
  "sentence_tokens" : [ "3.2", "." ]
}, {
  "sentence_tokens" : [ "To", "evaluate", "the", "effects", "of", "calphostin", "C", "and", "chelerythrine", "on", "the", "development", "of", "WNV", "replication", "complexes", ",", "the", "amount", "of", "dsRNA", "intermediates", ",", "a", "widely", "used", "maker", "of", "flavivirus", "replication", "complex", ",", "was", "assessed", "by", "immunofluorescence", "in", "WNV", "infected", "cells", "treated", ",", "or", "not", ",", "with", "the", "drugs", "." ]
}, {
  "sentence_tokens" : [ "As", "expected", ",", "no", "dsRNA", "was", "detected", "in", "mock", "infected", "cells", ",", "whereas", "it", "was", "observed", "in", "infected", "cells", "(", "Figure", "3a", ")", "." ]
}, {
  "sentence_tokens" : [ "A", "reduction", "of", "the", "fluorescence", "intensity", "was", "observed", "in", "infected", "cells", "treated", "with", "either", "drug", "(", "Figure", "3b", ")", ",", "being", "significantly", "lower", "in", "those", "treated", "with", "chelerythrine", ",", "thus", ",", "further", "confirming", "the", "impairment", "in", "WNV", "viral", "replication", "exerted", "by", "this", "PKC", "inhibitor", "." ]
}, {
  "sentence_tokens" : [ "4", "." ]
}, {
  "sentence_tokens" : [ "Discussion", "." ]
}, {
  "sentence_tokens" : [ "As", "currently", "no", "antiviral", "therapies", "against", "any", "flaviviruses", ",", "including", "WNV", ",", "have", "been", "approved", ",", "the", "search", "for", "inhibitory", "compounds", "is", "a", "keystone", "for", "therapeutic", "applications", "[", "39,40,41", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "this", "work", ",", "the", "effect", "of", "two", "broadly-active", "PKC", "inhibitors", ",", "calphostin", "C", ",", "a", "potent", "inhibitor", "used", "in", "the", "treatment", "of", "some", "cancer", "cell", "lines", "[", "42,43", "]", ",", "and", "chelerythrine", ",", "with", "broad-spectrum", "biological", "activities", ",", "such", "as", "antimicrobial", ",", "antifungal", ",", "anti-inflammatory", ",", "and", "anti-tumoral", "effects", "[", "44", "]", ",", "and", "two", "PKC", "inhibitors", "that", "have", "shown", "specific", "activity", "against", "conventional", "and", "novel", "PKC", "isoforms", ",", "sotrastaurine", "and", "enzastaurin", ",", "have", "been", "assayed", "against", "WNV", "infection", "in", "cell", "culture", "." ]
}, {
  "sentence_tokens" : [ "These", "differences", "could", "be", "probably", "related", "to", "their", "different", "mechanism", "of", "action", "and", "specificity", "for", "PKC", "isoforms", "." ]
}, {
  "sentence_tokens" : [ "Calphostin", "C", "interacts", "with", "the", "regulatory", "N-terminal", "domain", "of", "PKCs", "by", "competing", "for", "the", "binding", "site", "of", "DAG", "and", "phorbol", "esters", ",", "but", "not", "for", "Ca", ",", "while", "chelerythrine", "acts", "on", "the", "conserved", "catalytic", "C-terminal", "region", "[", "45", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "this", "sense", ",", "the", "lack", "of", "inhibition", "produced", "by", "sotrastaurine", "and", "enzastaurin", "point", "to", "atypical", "isoforms", "as", "a", "specific", "target", "for", "WNV", "infection.The", "role", "of", "PKCs", "has", "been", "studied", "in", "a", "wide", "variety", "of", "viruses", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "published", "results", "for", "flavivirus", "infections", "are", "controversial", "." ]
}, {
  "sentence_tokens" : [ "PKC", "phosphorylation", "of", "the", "C", "viral", "protein", "has", "been", "described", "as", "the", "key", "regulator", "of", "apoptosis", "induction", "during", "infection", "[", "25", "]", ",", "and", "inhibition", "of", "all", "PKC", "isoforms", "affected", "WNV", "entry", "into", "mosquito", "cells", "[", "26", "]", "." ]
}, {
  "sentence_tokens" : [ "Conversely", ",", "DENV", "replication", "was", "enhanced", "after", "in", "vitro", "inhibition", "with", "bisindlolylmaleinmide", "I", "(", "Bis", "I", ")", ",", "a", "specific", "chemical", "inhibitor", "of", "classical", "and", "novel", "PKCs", "[", "17", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "our", "study", ",", "both", "calphostin", "C", "and", "chelerythrine", "decreased", "the", "amount", "of", "RNA", "released", "to", "the", "culture", "medium", "and", "of", "dsRNA", "intermediates", "formation", ",", "even", "though", "the", "observed", "reduction", "was", "significantly", "higher", "when", "chelerythrine", "was", "used", "." ]
}, {
  "sentence_tokens" : [ "Thus", ",", "the", "specific", "relationships", "that", "occur", "between", "PKC", "isoforms", "and", "multiplication", "of", "viruses", "must", "be", "carefully", "addressed", "before", "future", "design", "of", "antiviral", "drugs", "to", "combat", "diseases", "that", "affect", "human", "and", "animal", "health", "." ]
}, {
  "sentence_tokens" : [ "Nonetheless", ",", "the", "results", "point", "that", "some", "PKC", "inhibitors", "(", "i.e", ".,", "chelerythrine", ")", "could", "become", "potential", "antiviral", "candidates", "for", "WNV", ",", "as", "well", "as", "for", "other", "related", "flaviviruses", "." ]
