  "sentence_tokens" : [ "To", "analyse", "the", "specific", "copper", "content", "of", "the", "purified", "protein", ",", "we", "digested", "the", "protein", "to", "release", "bound", "copper", "ions", "and", "subsequently", "used", "inductively", "coupled", "plasma", "mass", "spectrometry", "(", "ICP-MS", ")", "to", "measure", "the", "copper", "concentration", "." ]
}, {
  "sentence_tokens" : [ "The", "purified", "HEPH", "had", "an", "average", "of", "4.04", "copper", "atoms", "per", "protein", "molecule", "." ]
}, {
  "sentence_tokens" : [ "Compared", "with", "previously", "published", "HEPH", "purification", "protocols", ",", "our", "sample", "has", "a", "higher", "copper", "content", "than", "that", "of", "protein", "produced", "in", "BHK", "cells", "[", "16", "]", ",", "and", ",", "whilst", "it", "is", "similar", "to", "the", "copper", "content", "of", "protein", "generated", "in", "Pichia", "[", "14,17", "]", ",", "the", "yield", "is", "much", "higher", "(", "50", "-", "75", "mu", "/", "L", "vs.", "1", "-", "2", "mg/L", ")", "." ]
}, {
  "sentence_tokens" : [ "Curiously", ",", "the", "copper", "content", "in", "recombinant", "HEPH", "has", "consistently", "been", "shown", "to", "be", "lower", "than", "predicted", "." ]
}, {
  "sentence_tokens" : [ "As", "noted", "in", "previous", "studies", ",", "this", "could", "be", "due", "to", "incomplete", "copper", "loading", "of", "the", "expressed", "protein", ",", "or", "HEPH", "could", "have", "an", "intrinsically", "lower", "copper", "content", "than", "ceruloplasmin", "[", "14", "]", "." ]
}, {
  "sentence_tokens" : [ "Assuming", "an", "overall", "similar", "structure", "and", "mechanism", "to", "ceruloplasmin", "and", "other", "multicopper", "oxidases", ",", "a", "minimum", "of", "4", "copper", "ions", "would", "be", "required", "for", "activity", ",", "with", "one", "Type", "I", "copper", "site", "transferring", "electrons", "from", "the", "oxidation", "of", "Fe", "to", "the", "catalytic", "trinuclear", "cluster", "." ]
}, {
  "sentence_tokens" : [ "It", "is", "thus", "theoretically", "possible", "that", "HEPH", "only", "contains", "4", "intrinsic", "copper", "ions", ",", "which", "would", "correspond", "with", "the", "copper", "content", "of", "the", "recombinant", "preparations", "from", "both", "Pichia", "and", "the", "protein", "described", "here", "." ]
}, {
  "sentence_tokens" : [ "Supporting", "this", ",", "mutagenesis", "of", "the", "predicted", "substrate", "(", "iron", ")", "binding", "sites", "in", "the", "human", "HEPH", "protein", "--", "each", "in", "the", "proximity", "of", "one", "of", "the", "three", "predicted", "Type", "I", "copper", "sites", "--", "has", "indicated", "that", "only", "one", "site", "is", "functionally", "important", "[", "14", "]", ",", "whilst", "crystal", "soaking", "experiments", "have", "revealed", "that", "at", "least", "two", "sites", "are", "present", "in", "ceruloplasmin", "[", "37", "]", "." ]
}, {
  "sentence_tokens" : [ "In", "addition", ",", "although", "being", "a", "smaller", "protein", "and", "missing", "one", "of", "the", "copper", "containing", "domains", "of", "mammalian", "ferroxidases", ",", "the", "Fet3p", "protein", "from", "yeast", "provides", "a", "functional", "example", "of", "a", "ferroxidase", "with", "only", "4", "intrinsic", "copper", "ions", "[", "38", "]", "." ]
}, {
  "sentence_tokens" : [ "Oxidase", "activity", "and", "enzyme", "kinetic", "characterization", "of", "HEPH", "." ]
}, {
  "sentence_tokens" : [ "The", "oxidase", "activity", "of", "purified", "HEPH", "was", "established", "using", "both", "pPD-", "and", "Fz", "based", "assays", "where", "serum", "proteins", "from", "WT", "and", "CpKO", "mice", "were", "used", "as", "controls", "." ]
}, {
  "sentence_tokens" : [ "In", "the", "pPD", "assay", ",", "the", "absorbance", "monitored", "at", "530", "nm", "showed", "the", "rate", "of", "oxidation", "of", "pPD", "." ]
}, {
  "sentence_tokens" : [ "Here", ",", "15", "mu", "5", "purified", "HEPH", "had", "a", "similar", "activity", "to", "that", "of", "500", "mu", "5", "WT", "mouse", "serum", ",", "whereas", "increasing", "concentrations", "of", "HEPH", "yielded", "in", "higher", "activity", "(", "Fig", "3A", ")", "." ]
}, {
  "sentence_tokens" : [ "In", "contrast", ",", "the", "addition", "of", "D-penicillamine", ",", "a", "copper", "specific", "chelator", ",", "led", "to", "a", "drastic", "decrease", "in", "the", "oxidation", "of", "pPD", "(", "Fig", "3B", ")", "." ]
}, {
  "sentence_tokens" : [ "In", "the", "Fz", "assay", ",", "the", "oxidation", "of", "Fe", "to", "Fe", "was", "monitored", "as", "a", "decrease", "of", "the", "Fz-Fe", "complex", ",", "which", "absorbs", "at", "570", "nm", "." ]
}, {
  "sentence_tokens" : [ "Hence", ",", "in", "this", "assay", ",", "increasing", "concentrations", "of", "HEPH", "illustrated", "higher", "activity", "in", "the", "form", "of", "a", "greater", "decrease", "in", "A", ",", "indicative", "of", "the", "conversion", "of", "Fe", "to", "Fe", "(", "Fig", "3C", ")", "." ]
}, {
  "sentence_tokens" : [ "Similarly", "to", "the", "pPD", "assay", ",", "addition", "of", "D-penicillamine", "successfully", "inhibited", "this", "activity", "(", "Fig", "3D", ")", "." ]
}, {
  "sentence_tokens" : [ "As", "these", "assays", "clearly", "demonstrated", "the", "protein", "to", "be", "active", "and", "responsive", "to", "inhibitors", ",", "we", "also", "used", "a", "Fz", "based", "oxidase", "assay", "to", "determine", "the", "K", "and", "V", "of", "the", "purified", "protein", "(", "Fig", "3E", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "K", "for", "Fe", "was", "determined", "to", "be", "1.9", "mu", "M", ",", "which", "is", "similar", "to", "that", "of", "Fet3p", "(", "2.0", "-", "4.8", "mu", "M", ")", "[", "39,40", "]", ",", "HEPH", "purified", "from", "BHK", "(", "2.1", "mu", "M", ")", "[", "16", "]", "and", "Pichia", "cells", "(", "3.2", "mu", "M", ")", "[", "17", "]", "." ]
}, {
  "sentence_tokens" : [ "However", ",", "our", "protein", "has", "significantly", "higher", "V", "than", "the", "BHK", "and", "Pichia", "produced", "HEPH", "protein", "(", "4.1", "mu", "M", "min", "versus", "0.5", "and", "0.15", "mu", "M", "min", ",", "respectively", ")", "." ]
}, {
  "sentence_tokens" : [ "Compared", "to", "the", "BHK", "produced", "protein", ",", "the", "improved", "catalytic", "rate", "is", "likely", "a", "result", "of", "the", "higher", "copper", "content", "of", "the", "purified", "HEPH", "protein", "." ]
}, {
  "sentence_tokens" : [ "With", "an", "average", "of", "4", "copper", "ions", ",", "the", "protein", "is", "expected", "to", "have", "both", "a", "catalytic", "trinuclear", "copper", "site", "and", "a", "peripheral", "Type", "I", "site", "in", "place", ",", "similar", "to", "the", "Fet3p", "protein", "." ]
}, {
  "sentence_tokens" : [ "Indeed", ",", "the", "V", "of", "the", "purified", "HEPH", "protein", "is", "more", "similar", "to", "that", "of", "Fet3p", "(", "1.0", "-", "1.8", "mu", "M", "min", ")", "." ]
}, {
