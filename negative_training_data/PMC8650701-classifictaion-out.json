  "sentence_tokens" : [ "In", "this", "study", ",", "63", "patients", "with", "CRRT", "were", "divided", "into", "the", "survivor", "(", "34/63", ",", "54.0", "%", ")", "and", "non", "survivor", "groups", "(", "29/63", ",", "46.0", "%", ")", "." ]
}, {
  "sentence_tokens" : [ "The", "characteristics", ",", "perioperative", "factors", ",", "laboratory", "data", ",", "and", "outcomes", "of", "the", "patients", "with", "CRRT", "are", "shown", "in", "Table", "5", "." ]
