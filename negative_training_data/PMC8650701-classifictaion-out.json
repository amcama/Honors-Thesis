}, {
  "sentence_tokens" : [ "In", "our", "study", ",", "we", "found", "that", "AKI", "stage", "2", "-", "3", "was", "an", "independent", "risk", "factor", "for", "CRRT", "." ]
}, {
  "sentence_tokens" : [ "CRRT", "can", "maintain", "water", ",", "electrolyte", ",", "acid-base", "balance", ",", "uremic", "solute", "homeostasis", "to", "prevent", "or", "delay", "the", "deterioration", "of", "renal", "function", "(", "32", ")", "." ]
