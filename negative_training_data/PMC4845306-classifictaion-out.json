}, {
  "sentence_tokens" : [ "The", "experimental", "protocol", "was", "approved", "(", "approval", "number", ":", "ZJSR2014", "-", "0113", ")", "by", "the", "Institutional", "Animal", "Care", "and", "Use", "Committee", "(", "IACUC", ")", "at", "the", "Zhejiang", "Shuren", "University", "." ]
}, {
  "sentence_tokens" : [ "Experimental", "design", "." ]
}, {
  "sentence_tokens" : [ "The", "mice", "were", "allowed", "to", "acclimatize", "to", "the", "laboratory", "environment", "for", "1", "week", "prior", "to", "the", "experiments", "." ]
}, {
