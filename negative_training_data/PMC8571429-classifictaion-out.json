  "sentence_tokens" : [ "At", "the", "time", "of", "discharge", "examination", "from", "the", "rehabilitation", "center", ",", "although", "there", "was", "an", "improvement", "in", "overall", "functional", "status", "like", "sitting", "balance", ",", "independent", "bed", "mobility", ",", "improvement", "in", "lower", "limb", "spasticity", "but", "the", "sensations", "in", "lower", "limbs", "remained", "absent", "with", "power", "in", "lower", "limbs", "graded", "as", "0/5", "and", "neurogenic", "bladder", "." ]
