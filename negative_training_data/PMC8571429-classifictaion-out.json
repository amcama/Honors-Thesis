}, {
  "sentence_tokens" : [ "Conclusions", "." ]
}, {
  "sentence_tokens" : [ "This", "case", "series", "represents", "patients", "with", "COVID-19-associated", "acute", "transverse", "myelitis", ",", "a", "rare", "neurologic", "complication", "that", "was", "supported", "by", "abnormal", "MRI", "spinal", "cord", "and", "CSF", "findings", "." ]
