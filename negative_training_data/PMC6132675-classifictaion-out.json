  "sentence_tokens" : [ "The", "Oprm1", "A112G", "mouse", "exhibits", "genotype", "dependent", "molecular", "alterations", ",", "including", "reductions", "in", "MOR", "mRNA", "and", "protein", "expression", "(", "Huang", "et", "al.", "2012", ";", "Mague", "et", "al.", "2009", ";", "Wang", "et", "al.", "2014", ")", "as", "well", "as", "behavioral", "alterations", ",", "including", "reduced", "morphine", "(", "Huang", "et", "al.", "2012", ";", "Mague", "et", "al.", "2009", ";", "Wang", "et", "al.", "2014", ")", "and", "buprenorphine", "(", "Browne", "et", "al.", "2017", ")", "analgesia", ",", "and", "increased", "opioid", "self", "administration", "(", "Zhang", "et", "al.", "2015", ")", "." ]
