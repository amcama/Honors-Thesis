    "sentence_tokens" : [ "In", "that", "study", ",", "signs", "and", "symptoms", "of", "CNS", "involvement", "were", "reported", "in", "26", "(", "25", "%", ")", "patients", ",", "8", "of", "whom", "had", "generalized", "seizures", "." ]
