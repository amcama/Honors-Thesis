  }, {
    "sentence_tokens" : [ "Therefore", ",", "the", "aim", "of", "this", "study", "was", "to", "present", "data", "on", "the", "autonomic", "changes", "induced", "by", "precompetitive", "stress", "in", "mountain", "cyclists", "and", "their", "relation", "with", "physical", "fitness", "and", "anxiety", "." ]
  }, {
