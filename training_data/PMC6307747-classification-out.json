  }, {
    "sentence_tokens" : [ "And", "also", ",", "the", "interval", "between", "HRV", "collection", "time", "on", "the", "race", "day", "and", "the", "start", "of", "the", "competition", "(", "i.e.", "~", "30", "minutes", ")", "." ]
  }, {
    "sentence_tokens" : [ "Conclusion", "." ]
  }, {
    "sentence_tokens" : [ "A", "cycling", "competition", "is", "a", "stressful", "situation", "capable", "of", "altering", "autonomic", "parameters", ",", "normally", "requiring", "a", "considerable", "increase", "in", "sympathetic", "drive", "." ]
  }, {
    "sentence_tokens" : [ "Other", "markers", "of", "autonomic", "control", "evident", "in", "HRV", "data", "did", "not", "seem", "to", "respond", "strongly", "in", "the", "control", "vs", "pre-competition", "comparison", "." ]
  }, {
    "sentence_tokens" : [ "Body", "composition", "and", "aerobic", "capacity", ",", "indicators", "of", "physical", "fitness", ",", "presented", "a", "different", "relationship", "with", "HRV", ",", "while", "anxiety", "correlated", "with", "athletes", "'", "experience", "." ]
  }, {
    "sentence_tokens" : [ "Therefore", ",", "cyclists", "and", "their", "coaches", "need", "to", "work", "harder", "to", "control", "these", "indicators", "(", "i.e.", "body", "composition", "and", "aerobic", "capacity", ")", ",", "as", "well", "as", "broadening", "the", "participation", "of", "beginner", "athletes", "in", "important", "events", "in", "order", "to", "broaden", "their", "experience.New", "studies", "might", "seek", "to", "clarify", "how", "much", "previous", "experience", "contributes", "to", "the", "autonomic", "control", "of", "athletes", "in", "a", "more", "stressful", "situation", "." ]
