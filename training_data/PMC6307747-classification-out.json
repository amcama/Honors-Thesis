    "sentence_tokens" : [ "The", "autonomic", "changes", "between", "control", "and", "competition", "day", "are", "presented", "in", "Table", "2", "." ]
  }, {
    "sentence_tokens" : [ "Although", "there", "were", "mean", "changes", "in", "other", "autonomic", "variables", ",", "none", "achieved", "statistical", "significance.Regarding", "the", "participants", "'", "anxiety", "scores", ",", "self-confidence", ",", "the", "variable", "was", "presented", "the", "highest", "sum", "of", "anxiety", "domains", "evaluated", ",", "as", "can", "be", "seen", "in", "Table", "3", ",", "below.The", "participants", "'", "body", "composition", "correlated", "with", "the", "Delta", "SampEn", "(", "P", "=", "0.05", ")", "." ]
  }, {
    "sentence_tokens" : [ "The", "aerobic", "capacity", "also", "correlated", "with", "Delta", "RMSSD", "(", "P", "=", "0.05", ")", "." ]
  }, {
