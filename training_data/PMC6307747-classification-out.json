  }, {
    "sentence_tokens" : [ "Discussion", "." ]
  }, {
    "sentence_tokens" : [ "A", "cycling", "competition", "is", "a", "stressful", "situation", "capable", "of", "altering", "autonomic", "and", "hemodynamic", "parameters", "." ]
  }, {
    "sentence_tokens" : [ "In", "this", "study", ",", "we", "observed", "the", "increase", "of", "65", "%", "in", "SDNN", "(", "p", "=", "0.02", ")", "immediately", "prior", "to", "competition", ",", "which", "may", "reflect", "the", "increase", "in", "sympathetic", "autonomic", "control", "." ]
  }, {
    "sentence_tokens" : [ "A", "factor", "observed", "in", "a", "previous", "study", "[", "32", "]", "that", "used", "the", "secretion", "of", "salivary", "alpha-amylase", "(", "sAA", ")", "in", "endurance", "running", "(", "half", "marathon", ")", "and", "also", "characterized", "the", "increase", "of", "the", "adrenergic", "mechanism", ",", "which", "inflates", "the", "sympathetic", "activity.This", "may", "impact", "other", "physiological", "parameters", ",", "such", "as", "HR", "(", "18", "%", "increase", "[", "p", "=", "0.01", "]", ")", "." ]
  }, {
    "sentence_tokens" : [ "There", "were", "significant", "correlations", "between", "the", "physical", "fitness", "with", "some", "autonomic", "parameters", ",", "demonstrating", "that", "a", "better", "aerobic", "capacity", "led", "to", "a", "less", "parasympathetic", "withdrawal", "in", "relation", "to", "the", "control", "day", ",", "while", "anxiety", "correlated", "negatively", "with", "the", "years", "of", "experience", "in", "competition", "." ]
  }, {
    "sentence_tokens" : [ "Previous", "studies", "have", "shown", "that", "competitive", "anxiety", "is", "a", "mental", "state", "that", "includes", "cognitive", ",", "somatic", "and", "emotional", "components", "which", "may", "decrease", "athletic", "performance", "[", "33", ",", "34", ",", "35", ",", "36", "]", "." ]
  }, {
    "sentence_tokens" : [ "HRV", "changes", "have", "been", "demonstrated", "in", "previous", "studies", "in", "response", "to", "different", "acute", "exercise", "sessions", ",", "observing", "autonomic", "stress", "characterized", "by", "a", "vagal", "reduction", "and", "the", "increase", "in", "sympathetic", "nervous", "activity", "[", "37", ",", "38", "]", ",", "similar", "to", "what", "reported", "by", "a", "previous", "study", "that", "utilized", "other", "important", "stress", "sensitive", "biomarkers", "(", "i.e.", "cortisol", "and", "sAA", ")", "[", "32", "]", "." ]
  }, {
    "sentence_tokens" : [ "However", ",", "in", "relation", "to", "the", "pre-competitive", "stress", "in", "high-performance", "cycling", "athletes", ",", "there", "is", "little", "evidence.In", "our", "study", ",", "we", "found", "evidence", "of", "a", "decrease", "in", "parasympathetic", "activity", ",", "suggesting", "that", "a", "considerable", "amount", "of", "non", "linear", "behavior", "is", "by", "this", "branch", "of", "ANS", ",", "because", "non", "linear", "analysis", "can", "be", "used", "as", "a", "powerful", "tool", "for", "the", "description", "of", "biosynthetic", "characteristics", ",", "since", "they", "are", "able", "to", "reveal", "small", "differences", "in", "the", "behavior", "of", "the", "systems", ",", "similar", "to", "what", "was", "reported", "in", "previous", "studies", "[", "39", "]", "." ]
  }, {
    "sentence_tokens" : [ "Stress", "and", "anxiety", "are", "expected", "in", "a", "competitive", "situation", "[", "33", ",", "40", ",", "41", "]", "and", "may", "have", "been", "minimized", "by", "differentiated", "physical", "condition", "(", "BF", "9.86", "+/-", "4.60", "%", "and", "VO", "64.18", "+/-", "7.49", "ml/kg/min", ")", "[", "12", "]", "." ]
  }, {
    "sentence_tokens" : [ "The", "result", "of", "this", "competition", "stage", "would", "have", "little", "influence", "on", "the", "overall", "annual", "classification", ",", "and", "it", "did", "not", "qualify", "for", "another", "tournament", "." ]
  }, {
    "sentence_tokens" : [ "In", "this", "situation", ",", "anxiety", "did", "not", "correlate", "with", "HRV", "(", "all", "parameters", "evaluation", ")", ",", "though", ",", "it", "was", "clear", "that", "the", "cycling", "experience", "is", "a", "determinant", "that", "characterizes", "the", "athlete", "'s", "anxiety.The", "more", "experienced", "the", "athlete", "(", "i.e.", "years", "of", "training", ")", ",", "the", "less", "anxiety", "they", "appear", "to", "experience", "[", "41", "]", "." ]
  }, {
    "sentence_tokens" : [ "This", "finding", "reinforces", "previous", "studies", "that", "affirmed", "that", "experience", "with", "competition", "was", "a", "factor", "acting", "to", "minimize", "anxiety", "[", "41", ",", "42", "]", "." ]
  }, {
    "sentence_tokens" : [ "The", "aerobic", "capacity", "showed", "a", "negative", "correlation", "with", "the", "change", "in", "the", "RMSSD", "(", "i.e.", "Delta", "RMSSD", ")", "(", "p", "=", "0.05", ")", ",", "demonstrating", "that", "physical", "fitness", "should", "be", "considered", "when", "evaluating", "autonomic", "control", "[", "12", "]", "." ]
  }, {
    "sentence_tokens" : [ "This", "study", "had", "some", "limitations", "which", "should", "be", "considered", "." ]
  }, {
    "sentence_tokens" : [ "Firstly", ",", "the", "competition", "phase", "chosen", "for", "evaluation", "was", "not", "the", "most", "important", "of", "the", "season", "." ]
  }, {
    "sentence_tokens" : [ "Recent", "research", "has", "shown", "that", "pre-competitive", "stress", "is", "more", "evident", "in", "situations", "where", "the", "result", "really", "matters", "to", "the", "athlete", "[", "10", "]", "." ]
  }, {
